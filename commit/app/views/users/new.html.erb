<p> It Lives </p>

<%= form_for :user, url: users_path do |f| %>
    <p>
        <%= f.label :username %> <br>
        <%= f.text_field :username %>
    </p>
    
    <p>
        <%= f.label :email %> <br>
        <%= f.text_field :email %>
    </p>

    <p>
        <%= f.submit %>
    </p>
<% end %>
